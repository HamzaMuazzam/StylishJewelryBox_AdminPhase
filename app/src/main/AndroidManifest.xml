<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stylishjewelryboxadminphase">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".fcmAll.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.SeeDeliveryBoysActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity
            android:name=".activities.TotalOrdersActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".activities.UpdateItemActivity" />
        <activity android:name=".activities.UpdateChildActivity" />
        <activity android:name=".activities.AddNewChildCategoryActivity" />
        <activity android:name=".activities.UpdateCategoryActivity" />
        <activity android:name=".activities.UpdateMaterialActivity" />
        <activity android:name=".activities.UpdateActivity" />
        <activity android:name=".activities.AddNewItemActivity" />
        <activity android:name=".activities.NewCategoryActivity" />
        <activity android:name=".activities.NewMaterialActivity" />
        <activity
            android:name=".activities.AddActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".activities.OrderAssignmentActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".activities.SubCategoryActivity" />
        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".fcmAll.FcmMessageService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>